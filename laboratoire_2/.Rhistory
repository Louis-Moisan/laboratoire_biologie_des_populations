# Define server
server <- function(input, output) {
# Reactive expression for population calculation
population_data <- reactive({
time_steps <- 1:input$time_steps
population <- round(input$population_initiale * (input$taux_croissance ^ time_steps), digits = 0)
data.frame(Time = time_steps, Population = population)
})
# Render the plot
output$population_plot <- renderPlotly({
plot_ly(population_data(), x = ~Time, y = ~Population, type = 'scatter', mode = 'lines+markers') %>%
layout(title = "Trajectoire de population",
xaxis = list(title = "Temps (t)"),
yaxis = list(title = "Taille de la population (N)"))
})
# Render the table
output$population_table <- renderDataTable({
population_data()
})
}
